# Local Development Environment Variables
# This file should be copied to .env.local for security

# Environment
VITE_NODE_ENV=development
VITE_VERSION=2.0.0

# Feature Flags
VITE_FEATURE_ONBOARDING=true
VITE_FEATURE_MIC_FINDER=true
VITE_FEATURE_STATS_CHARTS=true
VITE_FEATURE_SETLIST_BUILDER=true
VITE_FEATURE_DATA_EXPORT=true
VITE_FEATURE_KEYBOARD_SHORTCUTS=true
VITE_FEATURE_COLLABORATION=false
VITE_FEATURE_ANALYTICS=false

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_CSV_DATA_URL=./coordinates.csv
VITE_FEEDBACK_EMAIL=support@micfinderapp.com

# Performance Settings
VITE_CHART_ANIMATION_DURATION=750
VITE_NOTIFICATION_DURATION=3000
VITE_DEBOUNCE_DELAY=300

# Debug Settings
VITE_DEBUG_ENABLED=true
VITE_LOG_LEVEL=debug
VITE_SHOW_PERFORMANCE_METRICS=true
