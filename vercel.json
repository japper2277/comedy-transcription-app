{
  "version": 2,
  "buildCommand": "npm run build && cp transcription-prototype/index.html dist/index.html && cp transcription-prototype/script.js dist/script.js",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/transcribe.py": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/calendar",
      "dest": "/set_list_Calendar.html"
    },
    {
      "src": "/react-demo",
      "dest": "/react-demo.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}