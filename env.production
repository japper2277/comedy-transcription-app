# Production Environment Variables
# Copy to .env.production for production builds

# Environment
VITE_NODE_ENV=production
VITE_VERSION=2.0.0

# Feature Flags
VITE_FEATURE_ONBOARDING=true
VITE_FEATURE_MIC_FINDER=true
VITE_FEATURE_STATS_CHARTS=true
VITE_FEATURE_SETLIST_BUILDER=true
VITE_FEATURE_DATA_EXPORT=true
VITE_FEATURE_KEYBOARD_SHORTCUTS=true
VITE_FEATURE_COLLABORATION=false
VITE_FEATURE_ANALYTICS=false

# API Configuration (Production URLs)
VITE_API_BASE_URL=https://api.micfinderapp.com
VITE_CSV_DATA_URL=./coordinates.csv
VITE_FEEDBACK_EMAIL=support@micfinderapp.com

# Performance Settings (Optimized for production)
VITE_CHART_ANIMATION_DURATION=500
VITE_NOTIFICATION_DURATION=2000
VITE_DEBOUNCE_DELAY=200

# Debug Settings (Disabled for production)
VITE_DEBUG_ENABLED=false
VITE_LOG_LEVEL=error
VITE_SHOW_PERFORMANCE_METRICS=false
