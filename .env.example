# Firebase Configuration
# Copy this file to .env.local and fill in your Firebase project details

# Firebase Project Configuration
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456789
REACT_APP_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Development Configuration
REACT_APP_USE_FIREBASE_EMULATOR=true
NODE_ENV=development

# Feature Flags
REACT_APP_ENABLE_COLLABORATION=true
REACT_APP_ENABLE_COMMENTS=true
REACT_APP_ENABLE_SHARING=true

# Comedy Transcription API Keys
GEMINI_API_KEY=your_gemini_api_key_here