{
  "name": "mic-calendar",
  "version": "2.0.0",
  "description": "A comprehensive calendar application for tracking comedy performances, discovering mic nights, and building setlists",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "cd tests && npm run test",
    "perf": "lhci collect && lhci assert",
    "perf:ci": "lhci autorun",
    "perf:upload": "lhci collect && lhci upload",
    "lighthouse": "lighthouse http://localhost:3003/react-demo.html --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless\"",
    "lint": "eslint js/**/*.js",
    "format": "prettier --write js/**/*.js css/**/*.css *.html",
    "serve": "python -m http.server 3000",
    "clean": "rm -rf dist",
    "deploy": "npm run build && npm run perf && npm run test:e2e"
  },
  "keywords": [
    "comedy",
    "calendar",
    "performance-tracking",
    "mic-finder",
    "setlist-builder",
    "comedy-tools"
  ],
  "author": "Mic Calendar Team",
  "license": "MIT",
  "homepage": "https://micfinderapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/mic-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/mic-calendar/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@lhci/cli": "^0.15.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.0",
    "lighthouse": "^12.8.1",
    "madge": "^8.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@sentry/react": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
    "@stripe/stripe-js": "^7.9.0",
    "@vitejs/plugin-react": "^5.0.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.1.0",
    "gtag": "^1.0.1",
    "immer": "^10.1.1",
    "mixpanel-browser": "^2.55.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-firebase-hooks": "5.1.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/",
    "js/",
    "css/",
    "coordinates.csv",
    "set_list_Calendar.html",
    "README.md"
  ]
}
